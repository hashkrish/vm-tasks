#include <stdio.h>
int main() { return 0; }
EOF
reverse.c
#include <stdio.h>
int main() {
  int rr; int n = 0; int r = rr; scanf("%d", &n);
  while (n != 0) { rr = n % 10; r = r * 10 + rr; n /= 10; }
  printf("%d", r); return 0;
}
EOF
